									"const response = pm.response.json();",
									"pm.globals.set(\"CREATEDOBJECTKEY\", response.key);",
									"pm.globals.set(\"CREATEDOBJECTTIMESTAMP\", response.timestamp);",
									"pm.test(\"Status code is 201\", function () { pm.response.to.have.status(201); });"
								],
